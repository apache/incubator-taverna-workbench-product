    <plugin>
        <provider>taverna.sf.net</provider>
        <identifier>net.sf.taverna.t2.workbench</identifier>
		<version>${project.version}-${buildNumber}</version>
        <name>Workbench (required)</name>
        <description>Taverna 2 workbench. Required for graphical interface.</description>
        <enabled>true</enabled>
        <repositories>
            ${m2Repository}
            <repository>http://www.mygrid.org.uk/maven/repository/</repository>
            <repository>http://www.mygrid.org.uk/maven/snapshot-repository/</repository>
             <repository>http://uk.maven.org/maven2/</repository>
            <repository>http://download.java.net/maven/2/</repository>
            <repository>http://www.soapui.org/repository/maven2/</repository>

		<!--  for provenance -->
		<repository>http://openprovenance.org/java/maven-releases/</repository>			
		<repository>http://repo.aduna-software.org/maven2/releases/</repository>
        </repositories>
        <profile>

            <dependency>
                <groupId>net.sf.taverna.t2.taverna-workbench</groupId>
                <artifactId>workbench-plugin</artifactId>
                <version>${project.version}</version>
            </dependency>
            
             <dependency>
                <groupId>net.sf.taverna.t2.taverna-commandline</groupId>
                <artifactId>taverna-commandline-common</artifactId>
                <version>${t2.taverna-commandline.version}</version>
            </dependency>
        </profile>
        <compatibility>
            <application>
                <version>${project.version}</version>
            </application>
        </compatibility>
    </plugin>
    
        <plugin>
        <provider>taverna.sf.net</provider>
