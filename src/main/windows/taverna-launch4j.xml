<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <jar>lib\prelauncher-2.4.jar</jar>
  <outfile>taverna.exe</outfile>
  <errTitle>Could not start Taverna</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl>http://www.taverna.org.uk/about/contact-us/</supportUrl>
  <customProcName>true</customProcName>
  <stayAlive>false</stayAlive>
  <manifest></manifest>
  <icon>launcher_logo.ico</icon>
  <singleInstance>
    <mutexName>taverna240</mutexName>
    <windowTitle>Taverna workbench 2.4.0</windowTitle>
  </singleInstance>
  <jre>
    <path></path>
    <minVersion>1.6.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <initialHeapSize>150</initialHeapSize>
    <initialHeapPercent>5</initialHeapPercent>
    <maxHeapSize>400</maxHeapSize>
    <maxHeapPercent>15</maxHeapPercent>
    <opt>-XX:MaxPermSize=140m</opt>
    <opt>-Draven.profile=file:conf/current-profile.xml</opt>
    <opt>-Djava.system.class.loader=net.sf.taverna.raven.prelauncher.BootstrapClassLoader</opt>
    <opt>-Dsun.swing.enableImprovedDragGesture</opt>
    <opt>-Dtaverna.startup=.</opt>
  </jre>
  <splash>
    <file>launcher_splashscreen.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>300</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
  <versionInfo>
    <fileVersion>2.4.0.0</fileVersion>
    <txtFileVersion>2.4.0</txtFileVersion>
    <fileDescription>Taverna workbench</fileDescription>
    <copyright>2002-2012 University of Manchester, UK</copyright>
    <productVersion>2.4.0.0</productVersion>
    <txtProductVersion>2.4.0</txtProductVersion>
    <productName>Taverna workbench</productName>
    <companyName>myGrid</companyName>
    <internalName>taverna</internalName>
    <originalFilename>taverna.exe</originalFilename>
  </versionInfo>
  <messages>
    <startupErr>An error occurred while starting Taverna,  or Taverna is taking longer than expected to start.</startupErr>
    <bundledJreErr>This application was configured to use a bundled Java Runtime Environment but the runtime is missing or corrupted.</bundledJreErr>
    <jreVersionErr>Taverna requires the Java Runtime Environment to be installed.</jreVersionErr>
    <launcherErr>The registry refers to a nonexistent Java Runtime Environment installation or the runtime is corrupted.</launcherErr>
    <instanceAlreadyExistsMsg>Taverna is already running.</instanceAlreadyExistsMsg>
  </messages>
</launch4jConfig>
